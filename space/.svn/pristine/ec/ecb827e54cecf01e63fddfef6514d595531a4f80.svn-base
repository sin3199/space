				 str += '<TD>' + this.member_nm + '</TD><TD>' +this.member_email + '</TD><TD>'+ this.member_pw + '</TD><TD>' + this.member_reg_dt + '</TD><TD>' + this.member_pno + '</TD><TD>' + '삭제' + '</TD>';
				 str += '</TR>';
			});
	    $(".table-body").append(str);
   	 	}
		,error: function(err) {
			console.log("에러"+err);
		}
	})
}

//<!--listCate정렬-->
function sortCate(data){
